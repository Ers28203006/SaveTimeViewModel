@model IEnumerable<SaveTime.Web.Admin.Models.EmployeeViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccountPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BranchAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BranchPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccountEmail)
        </th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.BranchPhone)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.BranchAddress)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.BranchPhone)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.AccountEmail)
    </td>
</tr>
}

</table>
